{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc"]
    },
    "source": {
      "include": ["src"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
      "plugins/markdown",
      "node_modules/@ckeditor/jsdoc-plugins/lib/export-fixer/export-fixer.js"
    ],
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": true
    },
    "opts": {
      "destination": "./doc/",
      "encoding": "utf8",
      "private": true,
      "recurse": true,
      "whitelist": ["Optional", "List", "Of", "Top", "Level", "Classes"],
      "title": ""
    }
  }